{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport { mdiCartOutline, mdiMagnify, mdiMenu } from '@mdi/js';\nimport MegaMenuComp from \"@/components/MegaMenuComp\";\nimport ProductComp from \"@/components/ProductComp.vue\";\nexport default {\n  name: \"HeaderComp\",\n  components: {\n    ProductComp,\n    MegaMenuComp\n  },\n  data: () => ({\n    icons: {\n      mdiCartOutline,\n      mdiMagnify,\n      mdiMenu\n    },\n    showMegaMenu: false,\n    search: '',\n    showSearchbarResult: false\n  }),\n  computed: {\n    products() {\n      return this.$store.state.products;\n    },\n\n    filteredList() {\n      return this.products.filter(person => {\n        const searchResultObj = { ...person,\n          title: person.title\n        };\n        return Object.keys(searchResultObj).some(key => ('' + searchResultObj[key]).toLowerCase().includes(this.search.toLowerCase()));\n      });\n    }\n\n  },\n  methods: {\n    goToCart() {\n      this.$router.push({\n        name: 'CartView'\n      });\n    }\n\n  },\n  watch: {\n    search() {\n      this.showSearchbarResult = this.search.length > 2 ? true : false;\n    }\n\n  },\n\n  mounted() {\n    console.log(this.filteredList);\n  }\n\n};","map":{"version":3,"mappings":";AA6EA;AACA;AACA;AAEA;EACAA,kBADA;EAEAC;IAAAC;IAAAC;EAAA,CAFA;EAGAC;IACAC;MACAC,cADA;MACAC,UADA;MACAC;IADA,CADA;IAIAC,mBAJA;IAKAC,UALA;IAMAC;EANA,EAHA;EAWAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;QACA;UACAC;QADA;QAGA,oCACAC,IADA,CACAC,oFADA;MAEA,CANA;IAOA;;EAZA,CAXA;EAyBAC;IACAC;MACA;QAAAnB;MAAA;IACA;;EAHA,CAzBA;EA8BAoB;IACAV;MACA;IACA;;EAHA,CA9BA;;EAmCAW;IACAC;EACA;;AArCA","names":["name","components","ProductComp","MegaMenuComp","data","icons","mdiCartOutline","mdiMagnify","mdiMenu","showMegaMenu","search","showSearchbarResult","computed","products","filteredList","title","some","key","methods","goToCart","watch","mounted","console"],"sourceRoot":"src/components","sources":["HeaderComp.vue"],"sourcesContent":["<template>\r\n  <header class=\"grey lighten-5 w-100\">\r\n    <nav class=\"container\">\r\n      <v-row class=\"align-center justify-space-between\">\r\n        <v-col lg=\"3\" cols=\"3\" class=\"d-flex justify-end order-2 order-lg-3\">\r\n          <v-btn class=\"radius-1 py-6 px-5\"\r\n                 elevation=\"1\"\r\n                 outlined\r\n                 plain\r\n                 text\r\n                 @click=\"goToCart\"\r\n          >\r\n            <v-icon color=\"purple darken-4\" aria-label=\"My Account\" role=\"img\" aria-hidden=\"false\">\r\n              mdi-cart-outline\r\n            </v-icon>\r\n            <span class=\"btn-content\">\r\n    سبد خرید\r\n          </span>\r\n          </v-btn>\r\n        </v-col>\r\n        <v-col class=\"order-1\" lg=\"2\" md=\"4\" cols=\"4\">\r\n          <router-link :to=\"{name: 'HomeView'}\"><img class=\"logo\" src=\"../assets/img/logo/majazite-officially-logo.png\" alt=\"logo\"></router-link>\r\n        </v-col>\r\n        <v-col lg=\"5\" cols=\"12\" class=\"flex-column order-lg-2 pt-0 pt-lg-2 order-3\">\r\n          <div class=\"d-flex align-start justify-center\">\r\n          <v-text-field\r\n              color=\"purple\"\r\n              v-model=\"search\"\r\n              class=\"pt-6 searchbar\"\r\n              label=\"جستجو در محصولات\"\r\n              outlined\r\n              clearable\r\n          ></v-text-field>\r\n\r\n          </div>\r\n          <div class=\"p-relative w-100\" v-if=\"showSearchbarResult\">\r\n            <div class=\"searchbar-result-products p-absolute grey lighten-5 p-absolute w-100\">\r\n              <ProductComp v-for=\"product in filteredList\" :key=\"product.id\" class=\"searchbar-product cursor-pointer\">\r\n                <h3 class=\"py-5 px-3\">{{product.title}}</h3>\r\n              </ProductComp>\r\n            </div>\r\n          </div>\r\n        </v-col>\r\n      </v-row>\r\n    </nav>\r\n    <div class=\"pt-0 grey lighten-5\">\r\n      <div class=\"container\">\r\n          <span class=\"cursor-pointer\" @click=\"showMegaMenu = !showMegaMenu\">\r\n             <v-icon color=\"purple\">mdi-menu</v-icon>\r\n            همه دسته بندی ها\r\n          </span>\r\n        <MegaMenuComp v-bind:show-mega-menu=\"showMegaMenu\"/>\r\n      </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n<style lang=\"scss\">\r\nheader {\r\n  .logo {\r\n    width: 100%;\r\n  }\r\n  .searchbar-result-products{\r\n    top:0;\r\n    z-index: 2;\r\n  }\r\n  .searchbar {\r\n    border-radius:1rem !important;\r\n  }\r\n\r\n  @media only screen and (max-width: 650px) {\r\n    .btn-content {\r\n      display: none !important;\r\n    }\r\n  }\r\n}\r\n</style>\r\n<script>\r\nimport {mdiCartOutline, mdiMagnify, mdiMenu} from '@mdi/js';\r\nimport MegaMenuComp from \"@/components/MegaMenuComp\";\r\nimport ProductComp from \"@/components/ProductComp.vue\";\r\n\r\nexport default {\r\n  name: \"HeaderComp\",\r\n  components: {ProductComp, MegaMenuComp},\r\n  data: () => ({\r\n    icons: {\r\n      mdiCartOutline, mdiMagnify, mdiMenu,\r\n    },\r\n    showMegaMenu: false,\r\n    search: '',\r\n    showSearchbarResult : false\r\n  }),\r\n  computed: {\r\n    products() {\r\n      return this.$store.state.products;\r\n    },\r\n    filteredList() {\r\n      return this.products.filter((person) => {\r\n        const searchResultObj = {...person,\r\n          title: person.title,\r\n        }\r\n        return Object.keys(searchResultObj)\r\n            .some(key => ('' + searchResultObj[key]).toLowerCase().includes(this.search.toLowerCase()))\r\n      })\r\n    },\r\n  },\r\n  methods:{\r\n    goToCart(){\r\n      this.$router.push({name:'CartView'})\r\n    },\r\n  },\r\n  watch: {\r\n    search(){\r\n      this.showSearchbarResult = this.search.length > 2 ? true : false;\r\n    }\r\n  },\r\n  mounted() {\r\n    console.log(this.filteredList)\r\n  }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}